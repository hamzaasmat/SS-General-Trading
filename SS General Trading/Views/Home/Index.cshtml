<script type="text/javascript">
	var interval;
	function startTicker(){
		// $("#news li:first").addClass("animated bounceOut");
		$("#news li:first").slideUp(function(){
			$(this).appendTo($("#news")).fadeIn().addClass("animated bounceInUp");
		})
	}

	function stopTicker(){
		clearInterval(interval);
	}
	$(document).ready(function(){
		interval = setInterval(startTicker, 3000);
		$("#news").hover(function(){
			stopTicker();
		}, function(){
			interval = setInterval(startTicker, 3000);
		});

	});


</script>